{
    "manifest_version": 3,
    "name": "Search Enhancer",
    "version": "1.0.0",
    "description": "Enhance Google search results by removing AI overview",
    "permissions": ["activeTab", "storage"],
    "host_permissions": ["*://www.google.com/*"],
    "content_scripts": [
        {
            "matches": ["*://www.google.com/search*"],
            "js": ["src/content.js"],
            "run_at": "document_start"
        }
    ],
    "action": {
        "default_popup": "src/popup.html",
        "default_title": "Search Enhancer"
    }
}
